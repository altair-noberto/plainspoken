---
import Header from '../components/Header.astro'
import Nav from '../components/Navbar.astro'
const {pageTitle} = Astro.props
---
<html>
<Header Title={pageTitle} />
<Nav selected={pageTitle.split(' ')[0]}/>
	<body>
		<div class="mainDiv">
			<slot />
		</div>
	</body>
	<script is:inline src="src/components/Scripts/onStart.js"></script>
	<script src="../components/Scripts/Font.js"></script>
	<script src="../components/Scripts/DarkLight.js"></script>
</html>